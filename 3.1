#include<stdio.h>

char OR(char a, char b)
{
    if(a=='F'&&b=='F')
    return 'F';
    else
    return'T';
}
char AND(char a, char b)
{
    if(a=='T'&&b=='T')
    return 'T';
    else
    return'F';
}

char NEG(char x)
{
    if(x=='F')
    return'T';
    else
    return 'F';
}
int main (void)
{
    char a,b;
    printf("Enter a,b value");
    scanf("%c %c",&a,&b);

    printf("%c %c    %c  %c %c %c ",a,b,OR(a,b),NEG(OR(a,b)),NEG(a),NEG(b),AND(NEG(a),NEG(b)));
    return 0;

}
