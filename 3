#include<stdio.h>




int AND(int a,int b)
{
    int r;
    return r=(a==1&&b==1)?1:0;
}
int OR(int a,int b)
{
    int r;
    return r=(a==0&&b==0)?0:1;
}
int NOT(int a)
{
    int r;
    return r=(a==0)?1:0;
}
int main(void)
{
   int a=0;
   int b=0;

        printf( "\na OR b is %d",OR(a,b));
        printf( "\na AND b is %d",AND(a,b));
        printf("\na NOT is %d",NOT(a));

}
